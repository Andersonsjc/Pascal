PROGRAM ELEMENTO;
USES CRT;
TYPE
    ELEMENT=ARRAY[1..10] OF INTEGER;
VAR
   A:ELEMENT;
   I:INTEGER;
BEGIN
     CLRSCR;
     WRITELN('ENTRE COM OS 10 NÈMEROS POR FAVOR CARA');
     FOR I:=1 TO 10 DO
     READLN(A[I]);
     FOR I:=1 TO 10 DO
     IF(A[I] = 30)
     THEN
     BEGIN
     WRITELN('ESTES SAO OS NUMEROS ENCONTRADOS');
     WRITELN(A[I]);
     END;
READKEY;
END.