PROGRAM ALUNOS;
USES CRT;
TYPE
    NOTA=ARRAY[1..6] OF INTEGER;
VAR
   A:NOTA;
   I:INTEGER;
BEGIN
     CLRSCR;
     WRITELN('ENTRE COM AS 5 NOTAS');
     FOR I:=1 TO 6 DO
     READLN(A[I]);
     FOR I:=1 TO 6 DO
     IF(A[I] < 5)
     THEN
     BEGIN
     WRITELN('ESTAS SAO AS NOTAS MENORES QUE 5');
     WRITELN(A[I]);
     END;
READKEY;
END.